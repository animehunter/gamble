raco scribble --htmls \
  --dest doc-build \
  --redirect-main http://docs.racket-lang.org \
  +m \
  ++style $CSSDIR/manual-style.css \
  ++style $CSSDIR/manual-racket.css \
  ++style $CSSDIR/manual-fonts.css \
  prob/scribblings/prob.scrbl
